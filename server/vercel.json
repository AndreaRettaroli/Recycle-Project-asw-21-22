{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "https://recycle-project-api.vercel.app:3000/socket.io/$1",
      "protocols": ["websocket"]
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
